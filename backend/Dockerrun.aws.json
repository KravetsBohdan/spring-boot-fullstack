{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "retail-api",
      "image": "bohdankravets/retail-api:latest",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "SPRING_DATASOURCE_URL",
          "value": "jdbc:postgresql://localhost:5432/customer"
        }
      ]
    }
  ]
}